      </el-col>\n\n          <el-col :span=\"12\" class=\"merchantimg\">\n            <el-row>\n              <el-tabs v-model=\"activeName\">\n                <el-tab-pane label=\"\" name=\"first\">\n                  <el-upload\n                    class=\"avatar-uploader\"\n                    ref=\"coverUpload\"\n                    list-type=\"picture-card\"\n                    :headers=\"headers\"\n                    :action=\"coverImageUrl\"\n                    :data=\"{id: form.id}\"\n                    :file-list=\"coverImg\"\n                    :on-preview=\"handlePicturePreview\"\n                    :on-success=\"handleUploadSuccess\"\n                    :on-remove=\"handleRemoveCover\"\n                    :before-upload=\"beforePictureUpload\">\n                    <i class=\"el-icon-plus\"></i>\n                    <div slot=\"tip\" class=\"el-upload__tip\">jpg/png1M</div>\n                  </el-upload>\n                </el-tab-pane>\n                <el-tab-pane label=\"\" name=\"second\">\n                  <el-upload\n                    class=\"\"\n                    list-type=\"picture\"\n                    :headers=\"headers\"\n                    :action=\"sliderImageUrl\"\n                    :data=\"{id: form.id}\"\n                    :file-list=\"sliderPic\"\n                    :on-preview=\"handlePicturePreview\"\n                    :on-success=\"handleUploadSliderSuccess\"\n                    :on-remove=\"handleRemoveSlider\"\n                    :before-upload=\"beforePictureUpload\">\n                    <el-button size=\"small\" type=\"primary\"></el-button>\n                    <div slot=\"tip\" class=\"el-upload__tip\">jpg/png1M</div>\n                  </el-upload>\n                </el-tab-pane>\n                <el-tab-pane label=\"\" name=\"third\">\n                    <!-- quill-editor -->\n                    <quill-editor ref=\"myTextEditor\" v-model=\"form.htmlIntroduction\" :options=\"editorOption\"></quill-editor>\n                </el-tab-pane>\n              </el-tabs>\n            </el-row>\n\n            <el-dialog v-model=\"dialogVisible\" size=\"tiny\">\n              <img width=\"100%\" :src=\"dialogImageUrl\" alt=\"\">\n            </el-dialog>\n          </el-col>\n        </el-form>\n      </el-row>\n    </div>\n\n    <el-dialog title=\"\" v-model=\"dialogMapVisible\" @close=\"closeMapDialog\">\n      <div>\n        <baidu-map class=\"bm-view\" :center=\"center\" :scroll-wheel-zoom=\"true\" :zoom=\"mapZoom\" @moving=\"syncCenter\"\n                   @moveend=\"syncCenter\" @zoomend=\"syncZoom\">\n          <bm-marker :position=\"center\" :dragging=\"true\"\n                     :label=\"{content: '', opts: {offset: {width: 20, height: -10}}}\" @dragend=\"getMapPoint\">\n          </bm-marker>\n          <bm-geolocation anchor=\"BMAP_ANCHOR_BOTTOM_RIGHT\" :showAddressBar=\"true\" :autoLocation=\"true\"/>\n          <bm-navigation anchor=\"BMAP_ANCHOR_TOP_RIGHT\"/>\n          <bm-city-list anchor=\"BMAP_ANCHOR_TOP_LEFT\"/>\n        </baidu-map>\n      </div>\n      <div slot=\"footer\" class=\"dialog-map-footer\">\n        <el-form :inline=\"true\" class=\"demo-form-inline\">\n          <el-form-item label=\"\">\n            <el-input v-model.number=\"center.lng\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"\">\n            <el-input v-model.number=\"center.lat\"></el-input>\n          </el-form-item>\n          <el-form-item>\n            <el-button @click=\"dialogMapVisible = false\"> </el-button>\n            <el-button type=\"primary\" @click=\"setMapPoint\"> </el-button>\n          </el-form-item>\n        </el-form>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\n  import * as SITEURL from '@/store/app-urls'\n  import { MERCHANT_INFO } from '@/store/mutation-types'\n  export default {\n    name: 'user',\n    data () {\n      return {\n        editorOption: {},\n        form: {\n          addr: ''\n        },\n        area: '',\n        marketing: [],\n        uploadUrl: '',\n        submitBtnDis: false,\n        coverImageUrl: SITEURL.MERCHANT_IMAGE_UPLOAD_URL,\n        sliderImageUrl: SITEURL.MERCHANT_SLIDER_IMG_URL,\n        searchRecommends: [],\n        coverImg: [{url: this.$store.state.merchant.coverImg}],\n        sliderPic: [],\n        categoryOptions: [],\n        tagOptions: [],\n        scaleOptions: [],\n        dialogImageUrl: '',\n        dialogVisible: false,\n        dialogMapVisible: false,\n        activeName: 'first',\n        uploadData: {},\n        isDelImg: true,\n        loading: false,\n        headers: { Authorization: 'Bearer {' + this.$store.state.token + '}' },\n        // \n        center: {lng: 104.089305, lat: 30.571226},\n        mapZoom: 5,\n        rules: {\n          companyName: [\n            { required: true, message: '', trigger: 'blur' },\n            { min: 3, message: '3', trigger: 'blur' }\n          ],\n          addr: [\n            { required: true, message: '', trigger: 'blur' }\n          ],\n          businessHours: [\n            { required: true, message: '', trigger: 'blur' }\n          ],\n          contacts: [\n            { required: true, message: '', trigger: 'blur' }\n          ],\n          phoneNumber: [\n            { required: true, message: '', trigger: 'blur' },\n            { min: 11, message: '11', trigger: 'blur' }\n          ],\n          remark: [\n            { required: true, message: '', trigger: 'blur' }\n          ],\n          introduction: [\n            { required: true, message: '', trigger: 'blur' },\n            { min: 3, message: '3', trigger: 'blur' }\n          ]\n        }\n      }\n    },\n    created: function () {\n      // vuex\n      this.form = this.$store.state.merchant\n      this.area = this.form.fullName\n      this.uploadData = { id: this.form.id }\n      if (!this.form.longitude) {\n        this.form.longitude = 104.089305\n      }\n      if (!this.form.latitude) {\n        this.form.latitude = 30.571226\n      }\n      this.getMerchantInfo()\n    },\n    methods: {\n      getMerchantInfo () {\n        this.$axios.get(SITEURL.MERCHANT_INFO + '/' + this.form.id).then(response => {\n          var sliders = response.data.data.merchantImg\n          for (var i in sliders) {\n            this.sliderPic.push({url: sliders[i].showImg})\n          }\n          this.categoryOptions = response.data.data.category\n          this.tagOptions = response.data.data.tags\n          this.scaleOptions = response.data.data.scale\n          this.form.introduction = response.data.data.introduction\n          this.form.htmlIntroduction = response.data.data.htmlIntroduction\n        }).catch(error => {\n          console.log(error)\n        })\n      },\n      onSubmit (formName) {\n        this.submitBtnDis = true\n        // \n        if (!this.form.area) {\n          this.$message.warning('')\n          this.submitBtnDis = false\n          return\n        }\n        if (this.$refs.coverUpload.uploadFiles[0].url === '' || this.$refs.coverUpload.uploadFiles[0].url === null) {\n          this.$message.warning('')\n          this.submitBtnDis = false\n          return\n        }\n        this.$refs[formName].validate((valid) => {\n          if (valid) {\n            this.loading = true\n            this.$store.commit(MERCHANT_INFO, this.form)\n            this.$axios.post(SITEURL.MERCHANT_EDIT_URL, this.form).then(response => {\n              this.loading = false\n              if (response.data.success) {\n                this.$message.success('' + response.data.message)\n              } else {\n                this.$message.error('' + response.data.message)\n              }\n              this.submitBtnDis = false\n              this.$router.go(-1)\n            }).catch(error => {\n              this.loading = false\n              this.submitBtnDis = false\n              this.$message.error('' + error)\n              this.$router.go(-1)\n            })\n          } else {\n            this.loading = false\n            console.log('error submit!!')\n            this.submitBtnDis = false\n          }\n        })\n      },\n      onCancel () {\n        this.$router.go(-1)\n      },\n      changeCategory (item) {\n        this.form.category = item\n      },\n      changeTag (item) {\n        this.form.tag = item\n      },\n      changeScale (item) {\n        this.form.merchantScale = item\n      },\n\n      queryAreaSearch (queryString, cb) {\n        var results = []\n        if (/^[\\u4e00-\\u9fa5]+$/.test(queryString) && queryString.length >= 3) {\n          this.$axios.get(SITEURL.AUTOCOMPLETE_AREA + '?text=' + queryString).then(response => {\n            results = response.data.data\n            //  callback \n            cb(results)\n          }).catch(error => {\n            console.log(error)\n          })\n        } else {\n          cb(results)\n        }\n      },\n      handleAreaSelect (item) {\n        this.form.area = item.id\n      },\n\n      // \n      handleRemoveCover (file, fileList) {\n        if (this.isDelImg) {\n          this.$axios.post(SITEURL.MERCHANT_IMAGE_DEL_URL, {id: this.form.id, type: 'hcover'}).then(response => {\n            if (response.data.success) {\n              this.$message.success('' + response.data.message)\n            } else {\n              this.$message.error('' + response.data.message)\n            }\n          }).catch(error => {\n            console.log(error)\n          })\n        }\n      },\n      handleRemoveSlider (file, fileList) {\n        this.$axios.post(SITEURL.MERCHANT_SLIDER_DEL_URL, {merchant: this.form.id, showImg: file.url}).then(response => {\n          if (response.data.success) {\n            this.$message.success('' + response.data.message)\n          } else {\n            this.$message.error('' + response.data.message)\n          }\n        }).catch(error => {\n          console.log(error)\n        })\n      },\n      handleUploadSuccess (response, file, fileList) {\n        if (response.success) {\n          this.coverImg = [{url: response.data.coverImg}]\n          this.form.coverImg = response.data.coverImg\n          this.$message.success('' + response.message)\n        } else {\n          this.$message.error('' + response.message)\n          fileList = []\n        }\n        if (fileList.length > 1) {\n          fileList.splice(0, 1)\n        }\n//        this.imageUrl = URL.createObjectURL(file.raw)\n      },\n      handleUploadSliderSuccess (response, file, fileList) {\n        if (response.success) {\n          this.sliderPic.push({url: response.data.merchantImg.showImg})\n          this.$message.success('' + response.message)\n        } else {\n          this.$message.error('' + response.message)\n        }\n      },\n      beforePictureUpload (file) {\n        const isJPG = file.type === 'image/jpeg' || file.type === 'image/jpg' || file.type === 'image/png'\n        const isLt2M = file.size / 1024 / 1024 < 1\n\n        if (!isJPG) {\n          this.$notify.error({title: '', message: ' JPG  PNG!'})\n        }\n        if (!isLt2M) {\n          this.$notify.error({title: '', message: ' 1MB!'})\n        }\n        if (isJPG && isLt2M) {\n          this.isDelImg = true\n        } else {\n          this.isDelImg = false\n        }\n        return isJPG && isLt2M\n      },\n      handlePicturePreview (file) {\n        this.dialogImageUrl = file.url\n        this.dialogVisible = true\n      },\n\n      // \n      handleCurrentChange (val) {\n        this.currentPage = val\n        console.log(`: ${val}`)\n      },\n\n      // \n      handleSelect (item) {\n        this.form.addr = item.value\n        this.form.longitude = item.location.lng\n        this.form.latitude = item.location.lat\n        this.center.lng = item.location.lng\n        this.center.lat = item.location.lat\n      },\n      // input\n      handleMapClick (e) {\n        const that = this\n        this.$http.jsonp(SITEURL.BAIDU_MAP_URL + '/geocoder/v2/?address=' + that.form.addr + '&output=json&ak=EH07OrMrG9wr6sK9mqRa8meYx3MSGgep')\n          .then(response => {\n            if (response.data.status === 0) {\n              var location = response.data.result.location\n              this.form.longitude = location.lng\n              this.form.latitude = location.lat\n            }\n          }, response => {\n            // error callback\n          })\n      },\n      showMap () {\n        this.dialogMapVisible = true\n        this.center.lng = this.form.longitude\n        this.center.lat = this.form.latitude\n      },\n      syncCenter (e) {\n        const {lng, lat} = e.target.getCenter()\n        this.center.lng = lng\n        this.center.lat = lat\n      },\n      syncZoom (e) {\n        this.zoom = e.target.getZoom()\n      },\n      getMapPoint (e) {\n        this.center = e.point\n      },\n      setMapPoint () {\n        this.dialogMapVisible = false\n        this.form.longitude = this.center.lng\n        this.form.latitude = this.center.lat\n      },\n      closeMapDialog () {\n        this.center.lng = this.form.longitude\n        this.center.lat = this.form.latitude\n      },\n      queryAddress (queryString, cb) {\n        const that = this\n        this.$http.jsonp(SITEURL.BAIDU_MAP_URL + '/place/v2/suggestion?query=' + queryString + '&region=&city_limit=false&output=json&ak=EH07OrMrG9wr6sK9mqRa8meYx3MSGgep')\n          .then(response => {\n            if (response.data.status === 0) {\n              var recommends = []\n              var address = response.data.result\n              for (var i = 0; i < address.length; i++) {\n//                console.log(address[i])\n                recommends.push({\n                  value: address[i].name + ' ' + address[i].city + address[i].district,\n                  location: address[i].location\n                })\n              }\n              that.searchRecommends = recommends\n            }\n          }, response => {\n            // error callback\n          })\n        var results = that.searchRecommends\n        //  callback \n        cb(results)\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .merchantedit {\n    display: block;\n    /*width: 45%;*/\n    border: 1px solid #eaeefb;\n    border-radius: 4px;\n    transition: .2s;\n    padding: 24px;\n    height: 840px;\n  }\n  .merchantimg {\n    padding-left: 50px;\n  }\n\n  .avatar-uploader .el-upload {\n    border: 1px dashed #d9d9d9;\n    border-radius: 6px;\n    cursor: pointer;\n    position: relative;\n    overflow: hidden;\n  }\n  .avatar-uploader .el-upload:hover {\n    border-color: #20a0ff;\n  }\n  .merchantimg .avatar-uploader-icon {\n    font-size: 28px;\n    color: #8c939d;\n    width: 178px;\n    height: 178px;\n    line-height: 178px;\n    text-align: center;\n  }\n  .avatar {\n    width: 178px;\n    height: 178px;\n    display: block;\n  }\n  .upload-title {\n    margin-top: 15px;\n    margin-bottom: 20px;\n  }\n  .inline-input {\n    width: 100%;\n  }\n  .bm-view {\n    width: 100%;\n    height: 400px;\n  }\n  .dialog-map-footer {\n    text-align: center;\n  }\n  .input-num {\n    width: 82px;\n  }\n  .quill-editor {\n    height: 650px;\n  }\n  .slider {\n    font-size: 14px;\n    color: #8492a6;\n    line-height: 44px;\n  }\n</style>\n\n\n\n// WEBPACK FOOTER //\n// MerchantEdit.vue?35f6f066","<template>\n  <div id=\"merchantlist\" class=\"content\">\n    <div class=\"breadcrumb\">\n      <el-breadcrumb separator=\"/\">\n        <el-breadcrumb-item></el-breadcrumb-item>\n        <el-breadcrumb-item></el-breadcrumb-item>\n        <el-breadcrumb-item></el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n    <div class=\"search\">\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\n        <el-form-item label=\"\">\n          <el-input class=\"inline-input\" v-model=\"formInline.search\" placeholder=\"||\"></el-input>\n        </el-form-item>\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"onSearch\" icon=\"search\"></el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n    <div class=\"contentlist\">\n      <template>\n        <el-table :data=\"tableData\" stripe border style=\"width: 100%\" :height=\"$store.state.tableHeight\" type=\"selection\" v-loading=\"loading\">\n          <el-table-column type=\"expand\">\n            <template scope=\"props\">\n              <el-form label-position=\"left\" inline class=\"demo-table-expand\">\n                <el-form-item label=\"\">{{ props.row.createDate }}</el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.contacts }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.phoneNumber }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.browseNumber }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.attentionNumber }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.hotelRating }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.scale }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.scaleByMerchant }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.scaleByUser }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.merchantGiveUserPointScale }}</span></el-form-item>\n                <el-form-item label=\"\"><span>{{ props.row.businessHours }}</span></el-form-item>\n              </el-form>\n            </template>\n          </el-table-column>\n\n          <el-table-column prop=\"realName\" label=\"\"></el-table-column>\n          <el-table-column prop=\"companyName\" label=\"\"></el-table-column>\n          <el-table-column prop=\"mobile\" label=\"\" width=\"130\"></el-table-column>\n          <el-table-column prop=\"categoryName\" label=\"\" width=\"70\"></el-table-column>\n          <e